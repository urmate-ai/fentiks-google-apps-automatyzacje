{
  "name": "woocommerce-automatization",
  "version": "1.0.0",
  "description": "Automatyzacja WooCommerce dla Google Apps Script",
  "main": "index.js",
  "scripts": {
    "push": "clasp push",
    "pull": "clasp pull",
    "deploy": "clasp deploy",
    "start": "clasp push --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.78"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@babel/preset-env": "^7.23.8",
    "babel-jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./__tests__/setupTests.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  }
}